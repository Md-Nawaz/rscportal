<table class = "table table-bordered table-striped">
   <thead>.
      <tr>
         <th>Name</th>
         <th>Download Link</th>
         <th> </th>
      </tr>
   </thead>
   
   <tbody>
      <% @contract.each do |contract| %>
         
         <tr>
            <td><%= contract.ownername %></td>
            <%= image_tag @contract.adproof.thumb.url %>
            <td><%= button_to "Delete",  contract, method: :delete, class: "btn btn-danger", confirm: "Are you sure that you wish to delete #{contract.ownername}?" %></td>
         </tr>
         
      <% end %>
   </tbody>
   
</table>